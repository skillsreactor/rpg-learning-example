{
  "name": "rpg-learning-example",
  "version": "1.0.0",
  "description": "A beginner friendly command line RPG game that serves as an introduction to Constructor functions, prototypes, CommonJS modules, native Node.js modules, promises, error-first style callbacks, object destructuring, arrow function expressions, ternary operators, and third-party npm modules.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skillsreactor/rpg-learning-example.git"
  },
  "keywords": [
    "beginner",
    "learning",
    "rpg",
    "cli"
  ],
  "author": "SkillsReactor LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skillsreactor/rpg-learning-example/issues"
  },
  "homepage": "https://github.com/skillsreactor/rpg-learning-example#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.1.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rpg_learning_example"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
